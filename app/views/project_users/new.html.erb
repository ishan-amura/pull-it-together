<div class="row" id="members">
	<nav class="white col s6 offset-s3">
		<div class="nav-wrapper">
			<form>
				<div class="input-field">
					<input id="search" class="search" type="search" required>
					<label for="search"><i class="material-icons teal-text">search</i></label>
					<i class="material-icons">close</i>
				</div>
			</form>
		</div>
	</nav>
	<div class="col s6 offset-s3">
		<ul class="collection list">
			<% @users.each do |user|%>
			<%= link_to(add_project_user_path(@project,user),method: :post) do%>
			<li class="collection-item">
				<span class="secondary-content">
					<i class="material-icons">add</i>
				</span>
				<h5 class="teal-text name"><%= user.name %></h5>
				<p class="grey-text email"><%= user.email %></p>
			</li>
			<% end %>
			<% end %>
		</ul>
	</div>
</div>
